{
    "name": "quantaforge/mail",
    "description": "The QuantaForge Mail package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "quantaforge/collections": "^1.0.0",
        "quantaforge/container": "^1.0.0",
        "quantaforge/contracts": "^1.0.0",
        "quantaforge/macroable": "^1.0.0",
        "quantaforge/support": "^1.0.0",
        "league/commonmark": "^2.2",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/mailer": "^6.2",
        "tijsverkoyen/css-to-inline-styles": "^2.2.5"
    },
    "autoload": {
        "psr-4": {
            "QuantaForge\\Mail\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (^3.235.5).",
        "symfony/http-client": "Required to use the Symfony API mail transports (^6.2).",
        "symfony/mailgun-mailer": "Required to enable support for the Mailgun mail transport (^6.2).",
        "symfony/postmark-mailer": "Required to enable support for the Postmark mail transport (^6.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
